{
  "name": "hodor-it-back",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "server:run": "cd dist && nodemon index.js",
    "server:build": "tsc -p tsconfig.json",
    "server": "npm run server:build && npm run server:run",
    "test": "jest"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/secure-session": "^8.1.0",
    "fastify": "^5.0.0",
    "fastify-session": "^5.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 30000,
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
